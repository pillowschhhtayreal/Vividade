local function createPlayersTabContent()
    clearContentArea()
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Name = "PlayersContent"
    scrollFrame.Size = UDim2.new(1, -20, 1, -20)
    scrollFrame.Position = UDim2.new(0, 10, 0, 10)
    scrollFrame.BackgroundTransparency = 1
    scrollFrame.BorderSizePixel = 0
    scrollFrame.ScrollBarThickness = 6
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    scrollFrame.Parent = content
    scrollFrame.ZIndex = 2
    local layout = Instance.new("UIListLayout")
    layout.Padding = UDim.new(0, 8)
    layout.SortOrder = Enum.SortOrder.Name
    layout.Parent = scrollFrame
    layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        scrollFrame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 20)
    end)
    local function getOutfitPrice(userId, label)
        task.spawn(function()
            local ok, result = pcall(function()
                local HttpService = game:GetService("HttpService")
                local MarketplaceService = game:GetService("MarketplaceService")
                local raw = game:HttpGet("https://avatar.roblox.com/v1/users/" .. tostring(userId) .. "/currently-wearing")
                local data = HttpService:JSONDecode(raw)
                local total = 0
                if data and data.assetIds then
                    for _, assetId in ipairs(data.assetIds) do
                        local infoOk, info = pcall(function()
                            return MarketplaceService:GetProductInfo(assetId)
                        end)
                        if infoOk and info and info.PriceInRobux then
                            total = total + info.PriceInRobux
                        end
                    end
                end
                return total
            end)
            if ok and type(result) == "number" then
                label.Text = "outfit price : " .. tostring(result) .. " robux"
            else
                label.Text = "outfit price : N/A"
            end
        end)
    end
    local function createPlayerCard(targetPlayer)
        if targetPlayer == player then return end
        local card = Instance.new("Frame")
