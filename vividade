    timeDropdown.Size = UDim2.new(1, -160, 0, 25)
    timeDropdown.Position = UDim2.new(0, 150, 0, 2)
    timeDropdown.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
    timeDropdown.Text = timeOfDay .. "  ▼"
    timeDropdown.TextColor3 = Color3.fromRGB(255, 255, 255)
    timeDropdown.Font = Enum.Font.GothamMedium
    timeDropdown.TextSize = 12
    timeDropdown.TextXAlignment = Enum.TextXAlignment.Left
    timeDropdown.AutoButtonColor = false
    timeDropdown.ZIndex = 3
    timeDropdown.Parent = timeFrame
    local timePadding = Instance.new("UIPadding")
    timePadding.PaddingLeft = UDim.new(0, 10)
    timePadding.PaddingRight = UDim.new(0, 10)
    timePadding.Parent = timeDropdown
    local timeCorner = Instance.new("UICorner")
    timeCorner.CornerRadius = UDim.new(0, 6)
    timeCorner.Parent = timeDropdown
    local timeStroke = Instance.new("UIStroke")
    timeStroke.Color = Color3.fromRGB(70, 100, 255)
    timeStroke.Transparency = 0.7
    timeStroke.Thickness = 1
    timeStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    timeStroke.Parent = timeDropdown
    local timeMenu = Instance.new("Frame")
    timeMenu.Size = UDim2.new(1, -160, 0, 0)
    timeMenu.Position = UDim2.new(0, 150, 0, 30)
    timeMenu.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    timeMenu.BorderSizePixel = 0
    timeMenu.ClipsDescendants = true
    timeMenu.Visible = false
    timeMenu.ZIndex = 10
    timeMenu.Parent = timeFrame
    local timeMenuCorner = Instance.new("UICorner")
    timeMenuCorner.CornerRadius = UDim.new(0, 6)
    timeMenuCorner.Parent = timeMenu
    local timeMenuStroke = Instance.new("UIStroke")
    timeMenuStroke.Color = Color3.fromRGB(70, 100, 255)
    timeMenuStroke.Transparency = 0.7
    timeMenuStroke.Thickness = 1
    timeMenuStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    timeMenuStroke.Parent = timeMenu
    local timeLayout = Instance.new("UIListLayout")
    timeLayout.SortOrder = Enum.SortOrder.LayoutOrder
    timeLayout.Parent = timeMenu
    local timeOpen = false
    local times = {"Day", "Night"}
    for _, time in ipairs(times) do
        local timeButton = Instance.new("TextButton")
        timeButton.Size = UDim2.new(1, 0, 0, 25)
        timeButton.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
        timeButton.BackgroundTransparency = 0.3
        timeButton.Text = time
        timeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        timeButton.Font = Enum.Font.GothamMedium
        timeButton.TextSize = 11
        timeButton.TextXAlignment = Enum.TextXAlignment.Left
        timeButton.AutoButtonColor = false
        timeButton.ZIndex = 11
        timeButton.Parent = timeMenu
        local timeBtnPadding = Instance.new("UIPadding")
        timeBtnPadding.PaddingLeft = UDim.new(0, 10)
        timeBtnPadding.Parent = timeButton
        timeButton.MouseEnter:Connect(function()
            timeButton.BackgroundTransparency = 0
        end)
        timeButton.MouseLeave:Connect(function()
            timeButton.BackgroundTransparency = 0.3
        end)
        timeButton.MouseButton1Click:Connect(function()
            timeOfDay = time
            timeDropdown.Text = time .. "  ▼"
            setTimeOfDay(time)
            TweenService:Create(timeMenu, TweenInfo.new(0.2), {Size = UDim2.new(1, -160, 0, 0)}):Play()
            task.wait(0.2)
            timeMenu.Visible = false
            timeOpen = false
        end)
    end
    timeDropdown.MouseButton1Click:Connect(function()
        timeOpen = not timeOpen
        if timeOpen then
            timeMenu.Visible = true
            TweenService:Create(timeMenu, TweenInfo.new(0.2), {Size = UDim2.new(1, -160, 0, #times * 25)}):Play()
        else
            TweenService:Create(timeMenu, TweenInfo.new(0.2), {Size = UDim2.new(1, -160, 0, 0)}):Play()
            task.wait(0.2)
            timeMenu.Visible = false
        end
    end)
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 40)
end
local function createSettingsTabContent()
    clearContentArea()
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Name = "SettingsContent"
    scrollFrame.Size = UDim2.new(1, -20, 1, -20)
    scrollFrame.Position = UDim2.new(0, 10, 0, 10)
    scrollFrame.BackgroundTransparency = 1
    scrollFrame.BorderSizePixel = 0
    scrollFrame.ScrollBarThickness = 6
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 300)
    scrollFrame.Parent = content
    scrollFrame.ZIndex = 2
    local layout = Instance.new("UIListLayout")
    layout.Padding = UDim.new(0, 10)
    layout.SortOrder = Enum.SortOrder.LayoutOrder
    layout.Parent = scrollFrame
    local title = Instance.new("TextLabel")
    title.Name = "Title"
    title.Size = UDim2.new(1, 0, 0, 30)
    title.BackgroundTransparency = 1
    title.Text = "Controls"
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.Font = Enum.Font.GothamBold
    title.TextSize = 18
    title.TextXAlignment = Enum.TextXAlignment.Left
    title.ZIndex = 3
    title.Parent = scrollFrame
    local openGuiFrame = Instance.new("Frame")
    openGuiFrame.Size = UDim2.new(1, 0, 0, 30)
    openGuiFrame.BackgroundTransparency = 1
    openGuiFrame.Parent = scrollFrame
    openGuiFrame.ZIndex = 3
    local openGuiLabel = Instance.new("TextLabel")
    openGuiLabel.Size = UDim2.new(0, 150, 1, 0)
    openGuiLabel.BackgroundTransparency = 1
    openGuiLabel.Text = "To Open GUI:"
    openGuiLabel.TextColor3 = Color3.fromRGB(220, 220, 255)
    openGuiLabel.Font = Enum.Font.GothamMedium
    openGuiLabel.TextSize = 14
    openGuiLabel.TextXAlignment = Enum.TextXAlignment.Left
    openGuiLabel.ZIndex = 3
    openGuiLabel.Parent = openGuiFrame
    local openGuiButton = Instance.new("TextButton")
    openGuiButton.Size = UDim2.new(0, 70, 0, 25)
    openGuiButton.Position = UDim2.new(1, -70, 0, 2)
    openGuiButton.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
    openGuiButton.Text = openGuiHotkey.Name
    openGuiButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    openGuiButton.Font = Enum.Font.GothamMedium
    openGuiButton.TextSize = 11
    openGuiButton.AutoButtonColor = false
    openGuiButton.ZIndex = 3
    openGuiButton.Parent = openGuiFrame
    local openGuiCorner = Instance.new("UICorner")
    openGuiCorner.CornerRadius = UDim.new(0, 6)
    openGuiCorner.Parent = openGuiButton
    local openGuiGlow = Instance.new("UIStroke")
    openGuiGlow.Color = Color3.fromRGB(255, 255, 255)
    openGuiGlow.Transparency = 0.7
    openGuiGlow.Thickness = 1
    openGuiGlow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    openGuiGlow.Parent = openGuiButton
    local settingOpenGui = false
    openGuiButton.MouseButton1Click:Connect(function()
        if settingOpenGui then return end
        settingOpenGui = true
        openGuiButton.Text = "..."
        openGuiButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
        local connection
        connection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
            if gameProcessed then return end
            if input.UserInputType == Enum.UserInputType.Keyboard then
                openGuiHotkey = input.KeyCode
                openGuiButton.Text = input.KeyCode.Name
                openGuiButton.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
                settingOpenGui = false
                connection:Disconnect()
            end
        end)
    end)
    local multiSelectFrame = Instance.new("Frame")
    multiSelectFrame.Size = UDim2.new(1, 0, 0, 40)
    multiSelectFrame.BackgroundTransparency = 1
    multiSelectFrame.Parent = scrollFrame
    multiSelectFrame.ZIndex = 3
