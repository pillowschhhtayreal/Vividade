    djHotkeyFrame.Size = UDim2.new(1, 0, 0, 30)
    djHotkeyFrame.BackgroundTransparency = 1
    djHotkeyFrame.Parent = scrollFrame
    djHotkeyFrame.ZIndex = 3
    local djHotkeyLabel = Instance.new("TextLabel")
    djHotkeyLabel.Size = UDim2.new(0, 150, 1, 0)
    djHotkeyLabel.BackgroundTransparency = 1
    djHotkeyLabel.Text = "Hotkey: Q"
    djHotkeyLabel.TextColor3 = Color3.fromRGB(220, 220, 255)
    djHotkeyLabel.Font = Enum.Font.GothamMedium
    djHotkeyLabel.TextSize = 14
    djHotkeyLabel.TextXAlignment = Enum.TextXAlignment.Left
    djHotkeyLabel.ZIndex = 3
    djHotkeyLabel.Parent = djHotkeyFrame
    local djHotkeyBtn = Instance.new("TextButton")
    djHotkeyBtn.Size = UDim2.new(1, -160, 0, 25)
    djHotkeyBtn.Position = UDim2.new(0, 150, 0, 2)
    djHotkeyBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
    djHotkeyBtn.Text = "SET"
    djHotkeyBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    djHotkeyBtn.Font = Enum.Font.GothamMedium
    djHotkeyBtn.TextSize = 12
    djHotkeyBtn.AutoButtonColor = false
    djHotkeyBtn.ZIndex = 3
    djHotkeyBtn.Parent = djHotkeyFrame
    local djHotkeyCorner = Instance.new("UICorner")
    djHotkeyCorner.CornerRadius = UDim.new(0, 6)
    djHotkeyCorner.Parent = djHotkeyBtn
    local djHotkeyStroke = Instance.new("UIStroke")
    djHotkeyStroke.Color = Color3.fromRGB(70, 100, 255)
    djHotkeyStroke.Transparency = 0.7
    djHotkeyStroke.Thickness = 1
    djHotkeyStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    djHotkeyStroke.Parent = djHotkeyBtn
    local settingDJHotkey = false
    djHotkeyBtn.MouseButton1Click:Connect(function()
        if settingDJHotkey then return end
        settingDJHotkey = true
        djHotkeyBtn.Text = "..."
        djHotkeyBtn.BackgroundColor3 = Color3.fromRGB(70, 100, 255)
        local conn
        conn = UserInputService.InputBegan:Connect(function(input, gp)
            if gp then return end
            if input.UserInputType == Enum.UserInputType.Keyboard then
                doubleJumpHotkey = input.KeyCode
                djHotkeyLabel.Text = "Hotkey: " .. input.KeyCode.Name:upper()
                djHotkeyBtn.Text = "SET"
                djHotkeyBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
                settingDJHotkey = false
                conn:Disconnect()
                if doubleJumpEnabled then
                    setupDoubleJump()
                end
            end
        end)
    end)
    djToggleBtn.MouseButton1Click:Connect(function()
        doubleJumpEnabled = not doubleJumpEnabled
        djToggleBtn.Text = doubleJumpEnabled and "ON" or "OFF"
        djToggleBtn.BackgroundColor3 = doubleJumpEnabled and Color3.fromRGB(70, 100, 255) or Color3.fromRGB(50, 50, 70)
        setupDoubleJump()
    end)
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 40)
end
local function createClientTabContent()
    clearContentArea()
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Name = "ClientContent"
    scrollFrame.Size = UDim2.new(1, -20, 1, -20)
    scrollFrame.Position = UDim2.new(0, 10, 0, 10)
    scrollFrame.BackgroundTransparency = 1
    scrollFrame.BorderSizePixel = 0
    scrollFrame.ScrollBarThickness = 6
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 600)
    scrollFrame.Parent = content
    scrollFrame.ZIndex = 2
    local layout = Instance.new("UIListLayout")
    layout.Padding = UDim.new(0, 10)
    layout.SortOrder = Enum.SortOrder.LayoutOrder
    layout.Parent = scrollFrame
    local espTitle = Instance.new("TextLabel")
    espTitle.Size = UDim2.new(1, 0, 0, 30)
    espTitle.BackgroundTransparency = 1
    espTitle.Text = "ESP Settings"
    espTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
