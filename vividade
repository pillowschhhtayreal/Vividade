        local TeleportService = game:GetService("TeleportService")
        TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId, player)
    end)
    local serverHopFrame = Instance.new("Frame")
    serverHopFrame.Size = UDim2.new(1, 0, 0, 30)
    serverHopFrame.BackgroundTransparency = 1
    serverHopFrame.Parent = scrollFrame
    serverHopFrame.ZIndex = 3
    local serverHopLabel = Instance.new("TextLabel")
    serverHopLabel.Size = UDim2.new(0, 150, 1, 0)
    serverHopLabel.BackgroundTransparency = 1
    serverHopLabel.Text = "Server Hop:"
    serverHopLabel.TextColor3 = Color3.fromRGB(220, 220, 255)
    serverHopLabel.Font = Enum.Font.GothamMedium
    serverHopLabel.TextSize = 14
    serverHopLabel.TextXAlignment = Enum.TextXAlignment.Left
    serverHopLabel.ZIndex = 3
    serverHopLabel.Parent = serverHopFrame
    local serverHopButton = Instance.new("TextButton")
    serverHopButton.Size = UDim2.new(0, 70, 0, 25)
    serverHopButton.Position = UDim2.new(1, -70, 0, 2)
    serverHopButton.BackgroundColor3 = Color3.fromRGB(70, 100, 255)
    serverHopButton.Text = "HOP"
    serverHopButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    serverHopButton.Font = Enum.Font.GothamMedium
    serverHopButton.TextSize = 11
    serverHopButton.AutoButtonColor = false
    serverHopButton.ZIndex = 3
    serverHopButton.Parent = serverHopFrame
    local serverHopCorner = Instance.new("UICorner")
    serverHopCorner.CornerRadius = UDim.new(0, 6)
    serverHopCorner.Parent = serverHopButton
    local serverHopGlow = Instance.new("UIStroke")
    serverHopGlow.Color = Color3.fromRGB(255, 255, 255)
    serverHopGlow.Transparency = 0.7
    serverHopGlow.Thickness = 1
    serverHopGlow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    serverHopGlow.Parent = serverHopButton
    serverHopButton.MouseButton1Click:Connect(function()
        local TeleportService = game:GetService("TeleportService")
        local HttpService = game:GetService("HttpService")
        local servers = {}
        local req = game:HttpGetAsync("https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Asc&limit=100")
        local body = HttpService:JSONDecode(req)
        if body and body.data then
            for i, v in pairs(body.data) do
                if v.id ~= game.JobId and v.playing < v.maxPlayers then
                    table.insert(servers, v.id)
                end
            end
            if #servers > 0 then
                TeleportService:TeleportToPlaceInstance(game.PlaceId, servers[math.random(1, #servers)], player)
            end
        end
    end)
    local closeScriptFrame = Instance.new("Frame")
    closeScriptFrame.Size = UDim2.new(1, 0, 0, 30)
    closeScriptFrame.BackgroundTransparency = 1
    closeScriptFrame.Parent = scrollFrame
    closeScriptFrame.ZIndex = 3
    local closeScriptLabel = Instance.new("TextLabel")
    closeScriptLabel.Size = UDim2.new(0, 150, 1, 0)
    closeScriptLabel.BackgroundTransparency = 1
    closeScriptLabel.Text = "Close Script:"
    closeScriptLabel.TextColor3 = Color3.fromRGB(220, 220, 255)
    closeScriptLabel.Font = Enum.Font.GothamMedium
    closeScriptLabel.TextSize = 14
    closeScriptLabel.TextXAlignment = Enum.TextXAlignment.Left
    closeScriptLabel.ZIndex = 3
    closeScriptLabel.Parent = closeScriptFrame
    local closeScriptButton = Instance.new("TextButton")
    closeScriptButton.Size = UDim2.new(0, 70, 0, 25)
    closeScriptButton.Position = UDim2.new(1, -70, 0, 2)
    closeScriptButton.BackgroundColor3 = Color3.fromRGB(235, 70, 70)
    closeScriptButton.Text = "CLOSE"
    closeScriptButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    closeScriptButton.Font = Enum.Font.GothamMedium
    closeScriptButton.TextSize = 11
    closeScriptButton.AutoButtonColor = false
    closeScriptButton.ZIndex = 3
    closeScriptButton.Parent = closeScriptFrame
    local closeScriptCorner = Instance.new("UICorner")
    closeScriptCorner.CornerRadius = UDim.new(0, 6)
    closeScriptCorner.Parent = closeScriptButton
    local closeScriptGlow = Instance.new("UIStroke")
    closeScriptGlow.Color = Color3.fromRGB(255, 255, 255)
    closeScriptGlow.Transparency = 0.7
    closeScriptGlow.Thickness = 1
    closeScriptGlow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    closeScriptGlow.Parent = closeScriptButton
    closeScriptButton.MouseButton1Click:Connect(function()
        scriptEnabled = false
        screenGui:Destroy()
    end)
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 40)
end
local function createMiscTabContent()
    clearContentArea()
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Name = "MiscContent"
    scrollFrame.Size = UDim2.new(1, -20, 1, -20)
    scrollFrame.Position = UDim2.new(0, 10, 0, 10)
    scrollFrame.BackgroundTransparency = 1
    scrollFrame.BorderSizePixel = 0
    scrollFrame.ScrollBarThickness = 6
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 800)
    scrollFrame.Parent = content
    scrollFrame.ZIndex = 2
    local layout = Instance.new("UIListLayout")
    layout.Padding = UDim.new(0, 10)
    layout.SortOrder = Enum.SortOrder.LayoutOrder
    layout.Parent = scrollFrame
    local aimbotTitle = Instance.new("TextLabel")
    aimbotTitle.Name = "AimbotTitle"
    aimbotTitle.Size = UDim2.new(1, 0, 0, 30)
    aimbotTitle.BackgroundTransparency = 1
    aimbotTitle.Text = "Aimbot Settings"
    aimbotTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    aimbotTitle.Font = Enum.Font.GothamBold
    aimbotTitle.TextSize = 18
    aimbotTitle.TextXAlignment = Enum.TextXAlignment.Left
    aimbotTitle.ZIndex = 3
    aimbotTitle.Parent = scrollFrame
    local toggleFrame = Instance.new("Frame")
    toggleFrame.Size = UDim2.new(1, 0, 0, 30)
    toggleFrame.BackgroundTransparency = 1
    toggleFrame.Parent = scrollFrame
    toggleFrame.ZIndex = 3
    local toggleLabel = Instance.new("TextLabel")
    toggleLabel.Size = UDim2.new(0, 150, 1, 0)
    toggleLabel.BackgroundTransparency = 1
    toggleLabel.Text = "Aimbot:"
    toggleLabel.TextColor3 = Color3.fromRGB(220, 220, 255)
    toggleLabel.Font = Enum.Font.GothamMedium
    toggleLabel.TextSize = 14
    toggleLabel.TextXAlignment = Enum.TextXAlignment.Left
    toggleLabel.ZIndex = 3
    toggleLabel.Parent = toggleFrame
    local toggleButton = Instance.new("TextButton")
    toggleButton.Size = UDim2.new(0, 50, 0, 25)
    toggleButton.Position = UDim2.new(1, -50, 0, 2)
    toggleButton.BackgroundColor3 = aimbotEnabled and Color3.fromRGB(70, 100, 255) or Color3.fromRGB(50, 50, 70)
    toggleButton.Text = aimbotEnabled and "ON" or "OFF"
    toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    toggleButton.Font = Enum.Font.GothamMedium
    toggleButton.TextSize = 12
    toggleButton.AutoButtonColor = false
    toggleButton.ZIndex = 3
    toggleButton.Parent = toggleFrame
    local toggleCorner = Instance.new("UICorner")
    toggleCorner.CornerRadius = UDim.new(0, 6)
    toggleCorner.Parent = toggleButton
    local toggleGlow = Instance.new("UIStroke")
    toggleGlow.Color = Color3.fromRGB(255, 255, 255)
    toggleGlow.Transparency = 0.7
    toggleGlow.Thickness = 1
    toggleGlow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    toggleGlow.Parent = toggleButton
    toggleButton.MouseButton1Click:Connect(function()
        aimbotEnabled = not aimbotEnabled
        toggleButton.Text = aimbotEnabled and "ON" or "OFF"
        toggleButton.BackgroundColor3 = aimbotEnabled and Color3.fromRGB(70, 100, 255) or Color3.fromRGB(50, 50, 70)
        updateAimbotSettings()
    end)
    local rangeFrame = Instance.new("Frame")
    rangeFrame.Size = UDim2.new(1, 0, 0, 50)
    rangeFrame.BackgroundTransparency = 1
    rangeFrame.Parent = scrollFrame
    rangeFrame.ZIndex = 3
    local rangeLabel = Instance.new("TextLabel")
    rangeLabel.Size = UDim2.new(0, 150, 0, 30)
    rangeLabel.BackgroundTransparency = 1
    rangeLabel.Text = "Range: " .. aimbotRange
    rangeLabel.TextColor3 = Color3.fromRGB(220, 220, 255)
    rangeLabel.Font = Enum.Font.GothamMedium
    rangeLabel.TextSize = 14
    rangeLabel.TextXAlignment = Enum.TextXAlignment.Left
    rangeLabel.ZIndex = 3
    rangeLabel.Parent = rangeFrame
    local sliderBg = Instance.new("Frame")
    sliderBg.Size = UDim2.new(1, -160, 0, 6)
    sliderBg.Position = UDim2.new(0, 150, 0, 12)
    sliderBg.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
    sliderBg.BorderSizePixel = 0
    sliderBg.ZIndex = 3
    sliderBg.Parent = rangeFrame
    local sliderBgCorner = Instance.new("UICorner")
    sliderBgCorner.CornerRadius = UDim.new(1, 0)
    sliderBgCorner.Parent = sliderBg
    local sliderFill = Instance.new("Frame")
    sliderFill.Size = UDim2.new(aimbotRange / 250, 0, 1, 0)
    sliderFill.BackgroundColor3 = Color3.fromRGB(70, 100, 255)
    sliderFill.BorderSizePixel = 0
    sliderFill.ZIndex = 4
    sliderFill.Parent = sliderBg
    local sliderFillCorner = Instance.new("UICorner")
    sliderFillCorner.CornerRadius = UDim.new(1, 0)
    sliderFillCorner.Parent = sliderFill
    local sliderBtn = Instance.new("TextButton")
    sliderBtn.Size = UDim2.new(0, 20, 0, 20)
    sliderBtn.Position = UDim2.new(aimbotRange / 250, -10, 0.5, -10)
    sliderBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    sliderBtn.Text = ""
    sliderBtn.AutoButtonColor = false
    sliderBtn.ZIndex = 5
    sliderBtn.Parent = sliderBg
    local sliderBtnCorner = Instance.new("UICorner")
    sliderBtnCorner.CornerRadius = UDim.new(1, 0)
    sliderBtnCorner.Parent = sliderBtn
    local sliderBtnGlow = Instance.new("UIStroke")
    sliderBtnGlow.Color = Color3.fromRGB(70, 100, 255)
    sliderBtnGlow.Transparency = 0.5
    sliderBtnGlow.Thickness = 2
    sliderBtnGlow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    sliderBtnGlow.Parent = sliderBtn
    local dragging = false
    sliderBtn.MouseButton1Down:Connect(function()
        dragging = true
    end)
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local mousePos = UserInputService:GetMouseLocation().X
            local sliderPos = sliderBg.AbsolutePosition.X
            local sliderSize = sliderBg.AbsoluteSize.X
            local value = math.clamp((mousePos - sliderPos) / sliderSize, 0, 1)
            aimbotRange = math.floor(value * 250)
            if aimbotRange < 1 then aimbotRange = 1 end
            rangeLabel.Text = "Range: " .. aimbotRange
            sliderFill.Size = UDim2.new(value, 0, 1, 0)
            sliderBtn.Position = UDim2.new(value, -10, 0.5, -10)
        end
    end)
    local hotkeyFrame = Instance.new("Frame")
    hotkeyFrame.Size = UDim2.new(1, 0, 0, 30)
    hotkeyFrame.BackgroundTransparency = 1
    hotkeyFrame.Parent = scrollFrame
    hotkeyFrame.ZIndex = 3
    local hotkeyLabel = Instance.new("TextLabel")
    hotkeyLabel.Size = UDim2.new(0, 150, 1, 0)
    hotkeyLabel.BackgroundTransparency = 1
    hotkeyLabel.Text = "Hotkey: " .. aimbotHotkey:upper()
    hotkeyLabel.TextColor3 = Color3.fromRGB(220, 220, 255)
    hotkeyLabel.Font = Enum.Font.GothamMedium
    hotkeyLabel.TextSize = 14
    hotkeyLabel.TextXAlignment = Enum.TextXAlignment.Left
    hotkeyLabel.ZIndex = 3
    hotkeyLabel.Parent = hotkeyFrame
    local hotkeyButton = Instance.new("TextButton")
    hotkeyButton.Size = UDim2.new(0, 50, 0, 25)
    hotkeyButton.Position = UDim2.new(1, -50, 0, 2)
    hotkeyButton.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
    hotkeyButton.Text = "SET"
    hotkeyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    hotkeyButton.Font = Enum.Font.GothamMedium
    hotkeyButton.TextSize = 12
    hotkeyButton.AutoButtonColor = false
    hotkeyButton.ZIndex = 3
    hotkeyButton.Parent = hotkeyFrame
    local hotkeyCorner = Instance.new("UICorner")
    hotkeyCorner.CornerRadius = UDim.new(0, 6)
    hotkeyCorner.Parent = hotkeyButton
    local hotkeyGlow = Instance.new("UIStroke")
    hotkeyGlow.Color = Color3.fromRGB(255, 255, 255)
    hotkeyGlow.Transparency = 0.7
    hotkeyGlow.Thickness = 1
    hotkeyGlow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    hotkeyGlow.Parent = hotkeyButton
    local settingHotkey = false
    hotkeyButton.MouseButton1Click:Connect(function()
        if settingHotkey then return end
        settingHotkey = true
        hotkeyButton.Text = "..."
        hotkeyButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
        local connection
        connection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
            if gameProcessed then return end
            if input.UserInputType == Enum.UserInputType.Keyboard then
                aimbotHotkey = input.KeyCode.Name:lower()
                hotkeyLabel.Text = "Hotkey: " .. input.KeyCode.Name:upper()
                hotkeyButton.Text = "SET"
                hotkeyButton.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
                settingHotkey = false
                connection:Disconnect()
            end
        end)
    end)
    local targetFrame = Instance.new("Frame")
    targetFrame.Size = UDim2.new(1, 0, 0, 70)
    targetFrame.BackgroundTransparency = 1
    targetFrame.Parent = scrollFrame
    targetFrame.ZIndex = 3
    local targetLabel = Instance.new("TextLabel")
    targetLabel.Size = UDim2.new(0, 150, 0, 30)
    targetLabel.BackgroundTransparency = 1
    targetLabel.Text = "Target:"
    targetLabel.TextColor3 = Color3.fromRGB(220, 220, 255)
    targetLabel.Font = Enum.Font.GothamMedium
    targetLabel.TextSize = 14
    targetLabel.TextXAlignment = Enum.TextXAlignment.Left
    targetLabel.ZIndex = 3
    targetLabel.Parent = targetFrame
    local function getTargetDisplayText()
        for _, t in ipairs(aimbotTargetPlayers) do
            if t:lower() == "all" then return "all  ▼" end
        end
        if #aimbotTargetPlayers == 1 then return aimbotTargetPlayers[1].."  ▼" end
        return #aimbotTargetPlayers.." players  ▼"
    end
    local targetDropdown = Instance.new("TextButton")
    targetDropdown.Size = UDim2.new(1, -160, 0, 28)
    targetDropdown.Position = UDim2.new(0, 150, 0, 0)
    targetDropdown.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
    targetDropdown.Text = getTargetDisplayText()
    targetDropdown.TextColor3 = Color3.fromRGB(255, 255, 255)
    targetDropdown.Font = Enum.Font.GothamMedium
    targetDropdown.TextSize = 12
    targetDropdown.TextXAlignment = Enum.TextXAlignment.Left
    targetDropdown.AutoButtonColor = false
    targetDropdown.ZIndex = 3
    targetDropdown.Parent = targetFrame
    local tdPad = Instance.new("UIPadding") tdPad.PaddingLeft = UDim.new(0,10) tdPad.Parent = targetDropdown
    local tdCorner = Instance.new("UICorner") tdCorner.CornerRadius = UDim.new(0,6) tdCorner.Parent = targetDropdown
    local tdStroke = Instance.new("UIStroke") tdStroke.Color = Color3.fromRGB(70,100,255) tdStroke.Transparency = 0.7 tdStroke.Thickness = 1 tdStroke.Parent = targetDropdown
    local targetInfo = Instance.new("TextLabel")
    targetInfo.Size = UDim2.new(1, -160, 0, 15)
    targetInfo.Position = UDim2.new(0, 150, 0, 32)
    targetInfo.BackgroundTransparency = 1
    targetInfo.Text = "Shift+Click to select multiple"
    targetInfo.TextColor3 = Color3.fromRGB(150, 150, 170)
    targetInfo.Font = Enum.Font.Gotham
    targetInfo.TextSize = 10
    targetInfo.TextXAlignment = Enum.TextXAlignment.Left
    targetInfo.ZIndex = 3
    targetInfo.Parent = targetFrame
    local targetMenu = Instance.new("ScrollingFrame")
    targetMenu.Size = UDim2.new(1, -160, 0, 0)
    targetMenu.Position = UDim2.new(0, 150, 0, 50)
    targetMenu.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    targetMenu.BorderSizePixel = 0
    targetMenu.ClipsDescendants = true
    targetMenu.Visible = false
    targetMenu.ScrollBarThickness = 4
    targetMenu.ZIndex = 20
    targetMenu.Parent = targetFrame
    local tmCorner = Instance.new("UICorner") tmCorner.CornerRadius = UDim.new(0,6) tmCorner.Parent = targetMenu
    local tmStroke = Instance.new("UIStroke") tmStroke.Color = Color3.fromRGB(70,100,255) tmStroke.Transparency = 0.7 tmStroke.Thickness = 1 tmStroke.Parent = targetMenu
    local tmLayout = Instance.new("UIListLayout") tmLayout.SortOrder = Enum.SortOrder.LayoutOrder tmLayout.Parent = targetMenu
    local targetDropOpen = false
    local targetBtns = {}
    local function isAimbotTargeted(name)
        for _, t in ipairs(aimbotTargetPlayers) do
            if t:lower() == name:lower() then return true end
        end
        return false
    end
    local function populateTargetList()
        for _, c in ipairs(targetMenu:GetChildren()) do
            if c:IsA("TextButton") then c:Destroy() end
        end
        targetBtns = {}
        local allItems = {{name = "all", display = "all"}}
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr ~= player then
                table.insert(allItems, {name = plr.Name, display = plr.DisplayName .. " (" .. plr.Name .. ")"})
            end
        end
        for _, item in ipairs(allItems) do
            local btn = Instance.new("TextButton")
            btn.Size = UDim2.new(1, 0, 0, 26)
            btn.BackgroundColor3 = isAimbotTargeted(item.name) and Color3.fromRGB(70,100,255) or Color3.fromRGB(30,30,40)
            btn.BackgroundTransparency = isAimbotTargeted(item.name) and 0.2 or 0.3
            btn.Text = item.display
            btn.TextColor3 = Color3.fromRGB(255, 255, 255)
            btn.Font = Enum.Font.GothamMedium
            btn.TextSize = 11
            btn.TextXAlignment = Enum.TextXAlignment.Left
            btn.AutoButtonColor = false
            btn.ZIndex = 21
            btn.Parent = targetMenu
            local p = Instance.new("UIPadding") p.PaddingLeft = UDim.new(0,10) p.Parent = btn
            targetBtns[item.name] = btn
            btn.MouseButton1Click:Connect(function()
                local shift = UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) or UserInputService:IsKeyDown(Enum.KeyCode.RightShift)
                if item.name == "all" then
                    aimbotTargetPlayers = {"all"}
                    for n, b in pairs(targetBtns) do
                        b.BackgroundColor3 = n == "all" and Color3.fromRGB(70,100,255) or Color3.fromRGB(30,30,40)
                        b.BackgroundTransparency = n == "all" and 0.2 or 0.3
                    end
                elseif shift then
                    for i, t in ipairs(aimbotTargetPlayers) do
                        if t == "all" then table.remove(aimbotTargetPlayers, i) break end
                    end
                    local found = false
                    for i, t in ipairs(aimbotTargetPlayers) do
                        if t == item.name then table.remove(aimbotTargetPlayers, i) found = true break end
                    end
                    if not found then table.insert(aimbotTargetPlayers, item.name) end
                    if #aimbotTargetPlayers == 0 then aimbotTargetPlayers = {"all"} end
                    for n, b in pairs(targetBtns) do
                        local sel = isAimbotTargeted(n)
                        b.BackgroundColor3 = sel and Color3.fromRGB(70,100,255) or Color3.fromRGB(30,30,40)
                        b.BackgroundTransparency = sel and 0.2 or 0.3
                    end
                else
                    aimbotTargetPlayers = {item.name}
                    for n, b in pairs(targetBtns) do
                        b.BackgroundColor3 = n == item.name and Color3.fromRGB(70,100,255) or Color3.fromRGB(30,30,40)
                        b.BackgroundTransparency = n == item.name and 0.2 or 0.3
                    end
                end
                targetDropdown.Text = getTargetDisplayText()
            end)
        end
        targetMenu.CanvasSize = UDim2.new(0, 0, 0, tmLayout.AbsoluteContentSize.Y)
    end
    targetDropdown.MouseButton1Click:Connect(function()
        targetDropOpen = not targetDropOpen
        if targetDropOpen then
            populateTargetList()
            targetMenu.Visible = true
            local h = math.min(150, #Players:GetPlayers() * 26 + 26)
            TweenService:Create(targetMenu, TweenInfo.new(0.2), {Size = UDim2.new(1,-160,0,h)}):Play()
        else
            TweenService:Create(targetMenu, TweenInfo.new(0.2), {Size = UDim2.new(1,-160,0,0)}):Play()
            task.wait(0.2)
            targetMenu.Visible = false
        end
    end)
    local hitboxTitle = Instance.new("TextLabel")
    hitboxTitle.Size = UDim2.new(1, 0, 0, 40)
    hitboxTitle.BackgroundTransparency = 1
    hitboxTitle.Text = "Hitbox Extender"
    hitboxTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    hitboxTitle.Font = Enum.Font.GothamBold
    hitboxTitle.TextSize = 18
    hitboxTitle.TextXAlignment = Enum.TextXAlignment.Left
    hitboxTitle.ZIndex = 3
    hitboxTitle.Parent = scrollFrame
    local hitboxToggleFrame = Instance.new("Frame")
    hitboxToggleFrame.Size = UDim2.new(1, 0, 0, 30)
    hitboxToggleFrame.BackgroundTransparency = 1
    hitboxToggleFrame.Parent = scrollFrame
    hitboxToggleFrame.ZIndex = 3
