            task.wait(0.2)
            dropdownMenu.Visible = false
            dropdownOpen = false
        end)
    end
    dropdownButton.MouseButton1Click:Connect(function()
        dropdownOpen = not dropdownOpen
        if dropdownOpen then
            dropdownMenu.Visible = true
            TweenService:Create(dropdownMenu, TweenInfo.new(0.2), {Size = UDim2.new(1, -160, 0, #items * 25)}):Play()
        else
            TweenService:Create(dropdownMenu, TweenInfo.new(0.2), {Size = UDim2.new(1, -160, 0, 0)}):Play()
            task.wait(0.2)
            dropdownMenu.Visible = false
        end
