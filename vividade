    hitboxToggleLabel.ZIndex = 3
    hitboxToggleLabel.Parent = hitboxToggleFrame
    local hitboxToggleButton = Instance.new("TextButton")
    hitboxToggleButton.Size = UDim2.new(0, 50, 0, 25)
    hitboxToggleButton.Position = UDim2.new(1, -50, 0, 2)
    hitboxToggleButton.BackgroundColor3 = hitboxEnabled and Color3.fromRGB(70, 100, 255) or Color3.fromRGB(50, 50, 70)
    hitboxToggleButton.Text = hitboxEnabled and "ON" or "OFF"
    hitboxToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    hitboxToggleButton.Font = Enum.Font.GothamMedium
    hitboxToggleButton.TextSize = 12
    hitboxToggleButton.AutoButtonColor = false
    hitboxToggleButton.ZIndex = 3
    hitboxToggleButton.Parent = hitboxToggleFrame
    local hitboxToggleCorner = Instance.new("UICorner")
    hitboxToggleCorner.CornerRadius = UDim.new(0, 6)
    hitboxToggleCorner.Parent = hitboxToggleButton
    local hitboxToggleGlow = Instance.new("UIStroke")
    hitboxToggleGlow.Color = Color3.fromRGB(255, 255, 255)
    hitboxToggleGlow.Transparency = 0.7
    hitboxToggleGlow.Thickness = 1
    hitboxToggleGlow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    hitboxToggleGlow.Parent = hitboxToggleButton
    hitboxToggleButton.MouseButton1Click:Connect(function()
        hitboxEnabled = not hitboxEnabled
        hitboxToggleButton.Text = hitboxEnabled and "ON" or "OFF"
        hitboxToggleButton.BackgroundColor3 = hitboxEnabled and Color3.fromRGB(70, 100, 255) or Color3.fromRGB(50, 50, 70)
        updateHitboxes()
    end)
    local hitboxSizeFrame = Instance.new("Frame")
    hitboxSizeFrame.Size = UDim2.new(1, 0, 0, 50)
    hitboxSizeFrame.BackgroundTransparency = 1
    hitboxSizeFrame.Parent = scrollFrame
    hitboxSizeFrame.ZIndex = 3
