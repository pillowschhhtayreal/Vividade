local function setupDoubleJump()
    if doubleJumpConnection then
        doubleJumpConnection:Disconnect()
        doubleJumpConnection = nil
    end
    if not doubleJumpEnabled then return end

    local hasDoubleJumped = false
    local canUse = true
    local mouse = player:GetMouse()

    local function performDoubleJump()
        if not canUse then return end
        local character = player.Character
        local humanoid = character and character:FindFirstChildOfClass("Humanoid")
        if humanoid and not hasDoubleJumped then
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            task.wait(0.1)
            for _, v in ipairs(player.Backpack:GetChildren()) do
                if v.Name == "C4" and v:FindFirstChild("RemoteEvent") then
                    v.Parent = player.Character
                    task.wait()
                    humanoid:ChangeState(Enum.HumanoidStateType.Freefall)
                    task.wait(0.1)
                    humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                    v.RemoteEvent:FireServer(mouse.Hit.LookVector)
                    v.Parent = player.Backpack
                end
            end
            task.wait(0.1)
            for _, v in ipairs(player.Backpack:GetChildren()) do
                if v.Name == "Grenade" and v:FindFirstChild("RemoteEvent") then
                    v.Parent = player.Character
                    task.wait()
                    humanoid:ChangeState(Enum.HumanoidStateType.Freefall)
                    task.wait(0.1)
                    humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                    v:Activate()
                    v.Parent = player.Backpack
                end
            end
            hasDoubleJumped = true
            canUse = false
        end
    end

    doubleJumpConnection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        if input.KeyCode == doubleJumpHotkey then
            performDoubleJump()
        end
    end)

    player.CharacterAdded:Connect(function()
        task.wait(1)
        hasDoubleJumped = false
        canUse = true
    end)
end

local function startFakeG2V2()
    fakeG2V2Enabled = true
    fakeG2Enabled = false
    local spinSpeed = 40
    local flingPower = 200
    local REQUIRED_TOOL_NAME = "Throwing Knife"
    local spinning = false
    local currentTool = nil
    local sidewaysForce = 350
    local upwardForce = 50
    local isEquipped = false
    local wallCheckDistance = 1
    local rightSlideSpeed = 200

    local function setupCharV2(char)
        char.ChildAdded:Connect(function(child)
            if child:IsA("Tool") and child.Name == REQUIRED_TOOL_NAME then
                spinning = true
                currentTool = child
                isEquipped = true
                task.spawn(function()
                    while spinning and currentTool and currentTool.Parent == char do
                        RunService.RenderStepped:Wait()
                        local root = char:FindFirstChild("HumanoidRootPart")
                        if root then
                            root.CFrame = root.CFrame * CFrame.Angles(0, math.rad(spinSpeed), 0)
                        end
                    end
                end)
                child.Unequipped:Connect(function()
                    isEquipped = false
                end)
                child.Equipped:Connect(function()
                    isEquipped = true
                end)
            end
        end)
        char.ChildRemoved:Connect(function(child)
            if child == currentTool then
                spinning = false
                currentTool = nil
                isEquipped = false
            end
        end)
        local humanoid = char:WaitForChild("Humanoid")
        local root = char:WaitForChild("HumanoidRootPart")
        humanoid.Jumping:Connect(function()
            if spinning and root then
                local sideways = root.CFrame.RightVector * flingPower
                root.AssemblyLinearVelocity = Vector3.new(sideways.X, root.AssemblyLinearVelocity.Y, sideways.Z)
            end
        end)
        local wallSliding = false
        local conn
        conn = RunService.RenderStepped:Connect(function()
            local state = humanoid:GetState()
            if state == Enum.HumanoidStateType.Freefall then
                local forward = root.CFrame.LookVector
                local ray = Ray.new(root.Position, forward * wallCheckDistance)
                local part = workspace:FindPartOnRay(ray, char)
                if part and part.CanCollide then
                    wallSliding = true
                    local rightVector = root.CFrame.RightVector
                    root.Velocity = rightVector * rightSlideSpeed + Vector3.new(0, root.Velocity.Y, 0)
                else
                    wallSliding = false
                end
            else
                wallSliding = false
            end
        end)
        humanoid.Died:Connect(function()
            if conn then conn:Disconnect() end
        end)
    end

    UserInputService.JumpRequest:Connect(function()
        if isEquipped and fakeG2V2Enabled then
            local character = player.Character
            if not character then return end
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if not humanoidRootPart then return end
            local camRight = workspace.CurrentCamera.CFrame.RightVector
            humanoidRootPart.Velocity = (camRight * sidewaysForce) + Vector3.new(0, upwardForce, 0)
        end
    end)

    if player.Character then setupCharV2(player.Character) end
    player.CharacterAdded:Connect(function(char)
        spinning = false
        currentTool = nil
        isEquipped = false
        setupCharV2(char)
    end)
end

local function getClosestPlayer()
    local closestPlayer = nil
    local shortestDistance = aimbotRange
    if not player.Character or not player.Character:FindFirstChild("Head") then
        return nil
    end
    for _, v in pairs(Players:GetPlayers()) do
        if v ~= player then
            local char = v.Character
            local head = char and char:FindFirstChild("Head")
            if not head then continue end
            local targetMatch = false
            for _, target in ipairs(aimbotTargetPlayers) do
                if target:lower() == "all" then targetMatch = true break end
            end
            if not targetMatch then
                for _, target in ipairs(aimbotTargetPlayers) do
                    if v.Name:lower() == target:lower() or v.DisplayName:lower() == target:lower() then
                        targetMatch = true break
                    end
                end
            end
            if targetMatch then
                local distance = (head.Position - player.Character.Head.Position).Magnitude
                if distance < shortestDistance then
                    closestPlayer = v
                    shortestDistance = distance
                end
            end
        end
    end
    return closestPlayer
end
local function setupAimbot()
    if aimbotConnection then
        aimbotConnection:Disconnect()
    end
    aimbotConnection = RunService.RenderStepped:Connect(function()
        if aimbotEnabled and player.Character and player.Character:FindFirstChild("Head") then
            local target = getClosestPlayer()
            if target and target.Character and target.Character:FindFirstChild("Head") then
                local camera = workspace.CurrentCamera
                camera.CFrame = CFrame.new(camera.CFrame.Position, target.Character.Head.Position)
            end
        end
    end)
end
local function updateAimbotSettings()
    if aimbotEnabled then
        setupAimbot()
    elseif aimbotConnection then
        aimbotConnection:Disconnect()
        aimbotConnection = nil
    end
end
local function applyGroundBoost()
    local character = player.Character
    if not character then return end
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if not humanoidRootPart or not humanoid then return end
    if humanoid.MoveDirection.Magnitude == 0 then
        return
    end
    local camera = workspace.CurrentCamera
    local cameraDirection = camera.CFrame.LookVector
    local speed = 20
    local maxSpeed = 150
    local acceleration = 10
    bodyVelocity = Instance.new("BodyVelocity")
    bodyVelocity.MaxForce = Vector3.new(1e5, 0, 1e5)
    bodyVelocity.Velocity = Vector3.zero
    bodyVelocity.Parent = humanoidRootPart
    local runService = game:GetService("RunService")
    boostConnection = runService.Heartbeat:Connect(function()
        if not character or not character.Parent then
            boostConnection:Disconnect()
            boostConnection = nil
            if bodyVelocity then
                bodyVelocity:Destroy()
                bodyVelocity = nil
            end
            return
        end
        local newCameraDirection = workspace.CurrentCamera.CFrame.LookVector
        speed = math.min(speed + acceleration, maxSpeed)
        bodyVelocity.Velocity = Vector3.new(newCameraDirection.X, 0, newCameraDirection.Z).Unit * speed
    end)
end
local function stopBoost()
    if boostConnection then
        boostConnection:Disconnect()
        boostConnection = nil
    end
    if bodyVelocity then
        bodyVelocity:Destroy()
        bodyVelocity = nil
    end
end
local function onCharacterAdded(character)
    character.ChildAdded:Connect(function(child)
        if child:IsA("Tool") and child.Name == fakeG2SelectedItem and fakeG2Enabled then
            applyGroundBoost()
        end
    end)
    character.ChildRemoved:Connect(function(child)
        if child:IsA("Tool") and child.Name == fakeG2SelectedItem then
            stopBoost()
        end
    end)
end
local function setupFakeG2()
    if fakeG2Enabled then
        if player.Character then
            onCharacterAdded(player.Character)
        end
        player.CharacterAdded:Connect(onCharacterAdded)
    else
        stopBoost()
    end
end
local function gotoPlayer(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
    local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") and targetHRP then
        player.Character.HumanoidRootPart.CFrame = targetHRP.CFrame * CFrame.new(0, 0, 3)
    end
end
local function flingPlayer(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
    local targetHRP = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
    local myChar = player.Character
    if not myChar or not myChar:FindFirstChild("HumanoidRootPart") or not targetHRP then return end
    myChar.HumanoidRootPart.CFrame = targetHRP.CFrame * CFrame.new(0, 0, 1)
    task.wait(0.05)
    local rootpart = myChar.HumanoidRootPart
    if rootpart:FindFirstChild("FlingBV") then
        rootpart.FlingBV:Destroy()
    end
    local bv = Instance.new("BodyAngularVelocity")
    bv.MaxTorque = Vector3.new(1, 1, 1) * math.huge
    bv.P = math.huge
    bv.AngularVelocity = Vector3.new(0, 9e5, 0)
    bv.Name = "FlingBV"
    bv.Parent = rootpart
    local chars = myChar:GetChildren()
    for _, v in next, chars do
        if v:IsA("BasePart") then
            v.CanCollide = false
            v.Massless = true
            v.Velocity = Vector3.new(0, 0, 0)
        end
    end
    local noclipConn
    noclipConn = RunService.Stepped:Connect(function()
        for _, v in next, chars do
            if v:IsA("BasePart") then v.CanCollide = false end
        end
    end)
    task.delay(1.5, function()
        if rootpart and rootpart:FindFirstChild("FlingBV") then
            rootpart.FlingBV:Destroy()
        end
        if noclipConn then noclipConn:Disconnect() end
    end)
end
local function createESPVisual(character, targetPlayer)
    if not character or character:FindFirstChild("ESPVisual") then return end
    local visu = Instance.new("Folder")
    visu.Name = "ESPVisual"
    visu.Parent = character
    local highlight = Instance.new("Highlight")
    highlight.Name = "Highlight"
    highlight.Parent = visu
    highlight.Adornee = character
    highlight.Enabled = false
    highlight.OutlineColor = ESPState.Color
    highlight.FillColor = ESPState.Color
    highlight.OutlineTransparency = 0
    highlight.FillTransparency = ESPState.Transparency
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    local billboard = Instance.new("BillboardGui")
    billboard.Name = "ESPBillboard"
    billboard.Size = UDim2.new(0, 200, 0, 80)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true
    billboard.Enabled = false
    billboard.Parent = visu
    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(1, 0, 1, 0)
    mainFrame.BackgroundTransparency = 1
    mainFrame.Parent = billboard
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Name = "NameLabel"
    nameLabel.Size = UDim2.new(1, 0, 0.33, 0)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.TextSize = 16
    nameLabel.TextColor3 = ESPState.Color
    nameLabel.TextStrokeTransparency = 0
    nameLabel.TextStrokeColor3 = Color3.new(0,0,0)
    nameLabel.Text = targetPlayer.Name
    nameLabel.Parent = mainFrame
    local healthLabel = Instance.new("TextLabel")
    healthLabel.Name = "HealthLabel"
    healthLabel.Size = UDim2.new(1, 0, 0.33, 0)
    healthLabel.Position = UDim2.new(0, 0, 0.33, 0)
    healthLabel.BackgroundTransparency = 1
    healthLabel.Font = Enum.Font.Gotham
    healthLabel.TextSize = 14
    healthLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
    healthLabel.TextStrokeTransparency = 0
    healthLabel.TextStrokeColor3 = Color3.new(0,0,0)
    healthLabel.Text = "HP: ?"
    healthLabel.Parent = mainFrame
    local distanceLabel = Instance.new("TextLabel")
    distanceLabel.Name = "DistanceLabel"
    distanceLabel.Size = UDim2.new(1, 0, 0.34, 0)
    distanceLabel.Position = UDim2.new(0, 0, 0.66, 0)
    distanceLabel.BackgroundTransparency = 1
    distanceLabel.Font = Enum.Font.Gotham
    distanceLabel.TextSize = 12
    distanceLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    distanceLabel.TextStrokeTransparency = 0
    distanceLabel.TextStrokeColor3 = Color3.new(0,0,0)
    distanceLabel.Text = "0m"
    distanceLabel.Parent = mainFrame
    local lookLineFolder = Instance.new("Folder")
    lookLineFolder.Name = "LookLine"
    lookLineFolder.Parent = visu
    for i = 1, ESPState.LookLineLength do
        local part = Instance.new("Part")
        part.Name = "LookLinePart"..i
        part.Anchored = true
        part.CanCollide = false
        part.CanTouch = false
        part.CanQuery = false
        part.Size = Vector3.new(0.15, 0.15, 0.15)
        part.Transparency = 1
        part.Color = ESPState.Color
        part.Material = Enum.Material.Neon
        part.Shape = Enum.PartType.Ball
        part.Parent = lookLineFolder
    end
    ESPTrackedCharacters[character] = {
        Visuals = visu,
        Player = targetPlayer,
        Highlight = highlight,
        Billboard = billboard,
        NameLabel = nameLabel,
        HealthLabel = healthLabel,
        DistanceLabel = distanceLabel,
        LookLineFolder = lookLineFolder
    }
end
local function cleanupESPVisual(character)
    local data = ESPTrackedCharacters[character]
    if data and data.Visuals and data.Visuals.Parent then
        data.Visuals:Destroy()
    end
    ESPTrackedCharacters[character] = nil
end
local function updateESPLookLine(character, lookLineFolder)
    if not character or not lookLineFolder then return end
    local head = character:FindFirstChild("Head")
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if not head or not humanoid or not ESPState.ShowLookLine then
        for _, part in ipairs(lookLineFolder:GetChildren()) do
            if part:IsA("BasePart") then part.Transparency = 1 end
        end
        return
    end
    local rootPart = character:FindFirstChild("HumanoidRootPart")
    local lookVector = (humanoid.MoveDirection.Magnitude > 0.1) and humanoid.MoveDirection.Unit
        or (rootPart and rootPart.CFrame.LookVector)
        or head.CFrame.LookVector
    for _, part in ipairs(lookLineFolder:GetChildren()) do
        if part:IsA("BasePart") and part.Name:match("LookLinePart") then
            local idx = tonumber(part.Name:match("LookLinePart(%d+)"))
            if idx then
                local dist = idx * ESPLookLineDistance
                part.Position = head.Position + (lookVector * dist)
                part.Color = ESPState.Color
                local tf = (idx - 1) / math.max(ESPState.LookLineLength - 1, 1)
                part.Transparency = 0.3 + tf * 0.7
                local sf = 1 - tf * 0.5
                part.Size = Vector3.new(0.15*sf, 0.15*sf, 0.15*sf)
            end
        end
    end
end
local function updateESPVisual(character)
    if not character then return end
    local data = ESPTrackedCharacters[character]
    if not data then return end
    if not ESPState.Enabled then
        data.Highlight.Enabled = false
        data.Billboard.Enabled = false
        return
    end
    local primaryPart = character:FindFirstChild("HumanoidRootPart") or character:FindFirstChild("Head")
    if not primaryPart then return end
    local lpChar = player.Character
    local lpRoot = lpChar and (lpChar:FindFirstChild("HumanoidRootPart") or lpChar:FindFirstChild("Head"))
    local distance = 0
    if lpRoot then
        distance = (primaryPart.Position - lpRoot.Position).Magnitude
        if distance > ESPState.Range then
            data.Highlight.Enabled = false
            data.Billboard.Enabled = false
            return
        end
    end
    local color = ESPState.Color
    data.Highlight.Enabled = ESPState.Glow
    data.Highlight.OutlineColor = color
    data.Highlight.FillColor = color
    data.Highlight.FillTransparency = ESPState.Transparency
    data.Highlight.Adornee = character
    data.Billboard.Enabled = ESPState.Name or ESPState.Health or ESPState.Distance
    data.Billboard.Adornee = primaryPart
    data.NameLabel.Visible = ESPState.Name
    data.NameLabel.TextColor3 = color
    data.NameLabel.Text = data.Player.DisplayName
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if humanoid then
        data.HealthLabel.Visible = ESPState.Health
        local hp = humanoid.Health / humanoid.MaxHealth
        data.HealthLabel.TextColor3 = Color3.new(1 - hp, hp, 0)
        data.HealthLabel.Text = "HP: "..math.floor(humanoid.Health).."/"..math.floor(humanoid.MaxHealth)
    else
        data.HealthLabel.Visible = false
    end
    data.DistanceLabel.Visible = ESPState.Distance
    data.DistanceLabel.Text = math.floor(distance).."m"
    if data.LookLineFolder then
        updateESPLookLine(character, data.LookLineFolder)
    end
end
local function initESPForPlayer(targetPlayer)
    if targetPlayer == player then return end
    if targetPlayer.Character then
        createESPVisual(targetPlayer.Character, targetPlayer)
    end
    targetPlayer.CharacterAdded:Connect(function(char)
        task.wait(0.3)
        cleanupESPVisual(char)
        createESPVisual(char, targetPlayer)
    end)
    targetPlayer.CharacterRemoving:Connect(function(char)
        cleanupESPVisual(char)
    end)
end
for _, plr in ipairs(Players:GetPlayers()) do
    initESPForPlayer(plr)
end
Players.PlayerAdded:Connect(initESPForPlayer)
Players.PlayerRemoving:Connect(function(plr)
    if plr.Character then cleanupESPVisual(plr.Character) end
end)
RunService.Heartbeat:Connect(function()
    for character, data in pairs(ESPTrackedCharacters) do
        if character.Parent then
            updateESPVisual(character)
        else
            cleanupESPVisual(character)
        end
    end
end)
local function createHitbox(targetPlayer)
    if not targetPlayer or not targetPlayer.Character then return end
    local char = targetPlayer.Character
    local hrp = char:FindFirstChild("HumanoidRootPart")
    if hrp then
        if hitboxParts[targetPlayer.Name] then
            hitboxParts[targetPlayer.Name]:Destroy()
        end
        local hitbox = Instance.new("Part")
        hitbox.Name = "Hitbox_" .. targetPlayer.Name
        hitbox.Size = Vector3.new(hitboxSize, hitboxSize, hitboxSize)
        hitbox.CFrame = hrp.CFrame
        hitbox.Transparency = 0.7
        hitbox.Color = hitboxColor
        hitbox.Material = Enum.Material.Neon
        hitbox.CanCollide = false
        hitbox.Anchored = false
        hitbox.Massless = true
        hitbox.Parent = char
        local weld = Instance.new("WeldConstraint")
        weld.Part0 = hrp
        weld.Part1 = hitbox
        weld.Parent = hitbox
        hitboxParts[targetPlayer.Name] = hitbox
    end
end
local function removeHitbox(playerName)
    if hitboxParts[playerName] then
        hitboxParts[playerName]:Destroy()
        hitboxParts[playerName] = nil
    end
end
local function isHitboxTargeted(plr)
    for _, t in ipairs(hitboxTargetPlayers) do
        if t:lower() == "all" then return true end
        if plr.Name:lower() == t:lower() or plr.DisplayName:lower() == t:lower() then return true end
    end
    return false
end
local function updateHitboxes()
    for name, part in pairs(hitboxParts) do
        part:Destroy()
    end
    hitboxParts = {}
    if not hitboxEnabled then return end
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= player and plr.Character and isHitboxTargeted(plr) then
            createHitbox(plr)
        end
    end
end
local function updateHitboxColor(newColor)
    hitboxColor = newColor
    for _, part in pairs(hitboxParts) do
        if part and part.Parent then
            part.Color = newColor
        end
    end
end
Players.PlayerAdded:Connect(function(plr)
    plr.CharacterAdded:Connect(function()
        task.wait(0.5)
        if hitboxEnabled and isHitboxTargeted(plr) then
            createHitbox(plr)
        end
    end)
end)
for _, plr in ipairs(Players:GetPlayers()) do
    if plr ~= player then
        plr.CharacterAdded:Connect(function()
            task.wait(0.5)
            if hitboxEnabled and isHitboxTargeted(plr) then
                createHitbox(plr)
            end
        end)
    end
end
Players.PlayerRemoving:Connect(function(plr)
    removeHitbox(plr.Name)
end)
local function createESPBox(character, playerName)
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    local hrp = character.HumanoidRootPart
    local billboard = Instance.new("BillboardGui")
    billboard.Name = "ESPBox"
    billboard.Adornee = hrp
    billboard.Size = UDim2.new(4, 0, 5, 0)
    billboard.AlwaysOnTop = true
    billboard.Parent = hrp
    local box = Instance.new("Frame")
    box.Size = UDim2.new(1, 0, 1, 0)
    box.BackgroundTransparency = 0.8
    box.BackgroundColor3 = espColor
    box.BorderSizePixel = 0
    box.Parent = billboard
    local stroke = Instance.new("UIStroke")
    stroke.Color = espColor
    stroke.Thickness = 2
    stroke.Parent = box
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Size = UDim2.new(1, 0, 0, 20)
    nameLabel.Position = UDim2.new(0, 0, 0, -25)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Text = playerName
    nameLabel.TextColor3 = espColor
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.TextSize = 14
    nameLabel.TextStrokeTransparency = 0.5
    nameLabel.Parent = billboard
    espBoxes[playerName] = billboard
end
local function removeESPBox(playerName)
    if espBoxes[playerName] then
        espBoxes[playerName]:Destroy()
        espBoxes[playerName] = nil
    end
end
local function shouldESPPlayer(playerName)
    if not espEnabled then return false end
    for _, target in ipairs(espTargetPlayers) do
        if target:lower() == "all" then
            return true
        end
    end
    for _, target in ipairs(espTargetPlayers) do
        if playerName:lower() == target:lower() then
            return true
        end
    end
    return false
end
local function updateESP()
    for name, box in pairs(espBoxes) do
        box:Destroy()
    end
    espBoxes = {}
    if not espEnabled then return end
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= player and plr.Character then
            if shouldESPPlayer(plr.Name) then
                createESPBox(plr.Character, plr.DisplayName)
            end
        end
    end
end
local function updateESPColor(newColor)
    espColor = newColor
    for _, billboard in pairs(espBoxes) do
        local box = billboard:FindFirstChildOfClass("Frame")
        if box then
            box.BackgroundColor3 = newColor
            local stroke = box:FindFirstChildOfClass("UIStroke")
            if stroke then
                stroke.Color = newColor
            end
        end
        local nameLabel = billboard:FindFirstChildOfClass("TextLabel")
        if nameLabel then
            nameLabel.TextColor3 = newColor
        end
    end
end
local function toggleShaders()
    local Lighting = game:GetService("Lighting")
    if shadersEnabled then
        local bloom = Instance.new("BloomEffect")
        bloom.Name = "CustomBloom"
        bloom.Intensity = 0.3
        bloom.Size = 20
        bloom.Threshold = 0.6
        bloom.Parent = Lighting
        local sunRays = Instance.new("SunRaysEffect")
        sunRays.Name = "CustomSunRays"
        sunRays.Intensity = 0.1
        sunRays.Spread = 0.5
        sunRays.Parent = Lighting
        local colorCorrection = Instance.new("ColorCorrectionEffect")
        colorCorrection.Name = "CustomColorCorrection"
        colorCorrection.Brightness = -0.05
        colorCorrection.Contrast = 0.15
        colorCorrection.Saturation = -0.1
        colorCorrection.TintColor = Color3.fromRGB(180, 180, 200)
        colorCorrection.Parent = Lighting
        local atmosphere = Instance.new("Atmosphere")
        atmosphere.Name = "CustomAtmosphere"
        atmosphere.Density = 0.4
        atmosphere.Offset = 0.5
        atmosphere.Color = Color3.fromRGB(150, 150, 170)
        atmosphere.Decay = Color3.fromRGB(100, 100, 120)
        atmosphere.Glare = 0.2
        atmosphere.Haze = 1.5
        atmosphere.Parent = Lighting
        local sky = Instance.new("Sky")
        sky.Name = "CustomSky"
        sky.SkyboxBk = "rbxassetid://11292343069"
        sky.SkyboxDn = "rbxassetid://11292343304"
        sky.SkyboxFt = "rbxassetid://11292343124"
        sky.SkyboxLf = "rbxassetid://11292343189"
        sky.SkyboxRt = "rbxassetid://11292343252"
        sky.SkyboxUp = "rbxassetid://11292343375"
        sky.Parent = Lighting
        Lighting.GlobalShadows = true
        Lighting.Brightness = 2
        Lighting.OutdoorAmbient = Color3.fromRGB(100, 100, 120)
        Lighting.Ambient = Color3.fromRGB(80, 80, 100)
    else
        if Lighting:FindFirstChild("CustomBloom") then
            Lighting.CustomBloom:Destroy()
        end
        if Lighting:FindFirstChild("CustomSunRays") then
            Lighting.CustomSunRays:Destroy()
        end
        if Lighting:FindFirstChild("CustomColorCorrection") then
            Lighting.CustomColorCorrection:Destroy()
        end
        if Lighting:FindFirstChild("CustomAtmosphere") then
            Lighting.CustomAtmosphere:Destroy()
        end
        if Lighting:FindFirstChild("CustomSky") then
            Lighting.CustomSky:Destroy()
        end
        Lighting.GlobalShadows = true
        Lighting.Brightness = 2
        Lighting.OutdoorAmbient = Color3.fromRGB(70, 70, 70)
        Lighting.Ambient = Color3.fromRGB(0, 0, 0)
    end
end
local function setTimeOfDay(time)
    local Lighting = game:GetService("Lighting")
    if time == "Night" then
        Lighting.ClockTime = 0
    else
        Lighting.ClockTime = 14
    end
end
local function toggleLowQuality()
    if lowQualityEnabled then
        settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("Texture") or obj:IsA("Decal") then
                obj.Transparency = 1
            end
        end
    else
        settings().Rendering.QualityLevel = Enum.QualityLevel.Automatic
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("Texture") or obj:IsA("Decal") then
                obj.Transparency = 0
            end
        end
    end
end
Players.PlayerAdded:Connect(function()
    task.wait(1)
    updateESP()
end)
Players.PlayerRemoving:Connect(function(plr)
    removeESPBox(plr.Name)
end)
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode.Name:lower() == aimbotHotkey and aimbotEnabled then
        if aimbotToggle then
            aimbotEnabled = not aimbotEnabled
        else
            aimbotEnabled = true
        end
        updateAimbotSettings()
    end
end)
UserInputService.InputEnded:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode.Name:lower() == aimbotHotkey and not aimbotToggle and aimbotEnabled then
        aimbotEnabled = false
        updateAimbotSettings()
    end
end)
local function clearContentArea()
    for _, child in pairs(content:GetChildren()) do
        if child.Name ~= "ContentBg" then
            child:Destroy()
        end
    end
end
