local function updateContentPosition()
    if isSidebarOpen then
        content.Position = UDim2.new(0,220,0,45)
        content.Size = UDim2.new(1,-220,1,-45)
    else
        content.Position = UDim2.new(0,64,0,45)
        content.Size = UDim2.new(1,-64,1,-45)
    end
end
