        headshot.BorderSizePixel = 0
        headshot.Image = "rbxthumb://type=AvatarHeadShot&id=" .. targetPlayer.UserId .. "&w=150&h=150"
        headshot.ZIndex = 4
        headshot.Parent = card
        local headshotCorner = Instance.new("UICorner")
        headshotCorner.CornerRadius = UDim.new(1, 0)
        headshotCorner.Parent = headshot
        local headshotStroke = Instance.new("UIStroke")
        headshotStroke.Color = Color3.fromRGB(120, 255, 120)
        headshotStroke.Thickness = 2.5
        headshotStroke.Parent = headshot
        local infoFrame = Instance.new("Frame")
        infoFrame.Size = UDim2.new(0, 240, 1, -20)
        infoFrame.Position = UDim2.new(0, 110, 0, 10)
        infoFrame.BackgroundTransparency = 1
        infoFrame.ZIndex = 4
        infoFrame.Parent = card
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(1, 0, 0, 26)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = "name : " .. targetPlayer.DisplayName
        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.TextSize = 15
        nameLabel.TextXAlignment = Enum.TextXAlignment.Left
        nameLabel.ZIndex = 4
        nameLabel.Parent = infoFrame
        local ageLabel = Instance.new("TextLabel")
        ageLabel.Size = UDim2.new(1, 0, 0, 22)
        ageLabel.Position = UDim2.new(0, 0, 0, 28)
        ageLabel.BackgroundTransparency = 1
        ageLabel.Text = "account age : " .. targetPlayer.AccountAge .. " days"
        ageLabel.TextColor3 = Color3.fromRGB(190, 190, 210)
        ageLabel.Font = Enum.Font.Gotham
        ageLabel.TextSize = 13
        ageLabel.TextXAlignment = Enum.TextXAlignment.Left
        ageLabel.ZIndex = 4
        ageLabel.Parent = infoFrame
        local outfitLabel = Instance.new("TextLabel")
        outfitLabel.Size = UDim2.new(1, 0, 0, 22)
        outfitLabel.Position = UDim2.new(0, 0, 0, 52)
        outfitLabel.BackgroundTransparency = 1
        outfitLabel.Text = "outfit price : loading..."
        outfitLabel.TextColor3 = Color3.fromRGB(190, 190, 210)
        outfitLabel.Font = Enum.Font.Gotham
        outfitLabel.TextSize = 13
        outfitLabel.TextXAlignment = Enum.TextXAlignment.Left
        outfitLabel.ZIndex = 4
        outfitLabel.Parent = infoFrame
        getOutfitPrice(targetPlayer.UserId, outfitLabel)
        local sep = Instance.new("Frame")
        sep.Size = UDim2.new(0, 1, 1, -20)
        sep.Position = UDim2.new(1, -108, 0, 10)
        sep.BackgroundColor3 = Color3.fromRGB(70, 70, 90)
        sep.BorderSizePixel = 0
        sep.ZIndex = 4
        sep.Parent = card
        local btnCol = Instance.new("Frame")
        btnCol.Size = UDim2.new(0, 90, 1, -20)
        btnCol.Position = UDim2.new(1, -100, 0, 10)
        btnCol.BackgroundTransparency = 1
        btnCol.ZIndex = 4
        btnCol.Parent = card
        local function makeBtn(text, yPos, clickFn)
            local btn = Instance.new("TextButton")
            btn.Size = UDim2.new(1, 0, 0, 26)
            btn.Position = UDim2.new(0, 0, 0, yPos)
            btn.BackgroundColor3 = Color3.fromRGB(35, 35, 48)
            btn.Text = text
            btn.TextColor3 = Color3.fromRGB(240, 240, 255)
            btn.Font = Enum.Font.GothamBold
            btn.TextSize = 13
            btn.AutoButtonColor = false
            btn.ZIndex = 5
            btn.Parent = btnCol
            local c = Instance.new("UICorner")
            c.CornerRadius = UDim.new(0, 5)
            c.Parent = btn
            local s = Instance.new("UIStroke")
            s.Color = Color3.fromRGB(255, 255, 255)
            s.Transparency = 0.8
            s.Thickness = 1
            s.Parent = btn
            btn.MouseEnter:Connect(function()
                btn.BackgroundColor3 = Color3.fromRGB(55, 55, 75)
            end)
            btn.MouseLeave:Connect(function()
                btn.BackgroundColor3 = Color3.fromRGB(35, 35, 48)
            end)
            btn.MouseButton1Click:Connect(clickFn)
            return btn
        end
        makeBtn("GOTO", 15, function()
            gotoPlayer(targetPlayer)
        end)
        makeBtn("FLING", 48, function()
            flingPlayer(targetPlayer)
        end)
    end
    for _, plr in ipairs(Players:GetPlayers()) do
        createPlayerCard(plr)
    end
    Players.PlayerAdded:Connect(function(plr)
        task.wait(0.5)
        createPlayerCard(plr)
    end)
    Players.PlayerRemoving:Connect(function(plr)
        local c = scrollFrame:FindFirstChild(plr.Name)
        if c then c:Destroy() end
    end)
end
