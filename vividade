    local hitboxDragging = false
    hitboxSliderBtn.MouseButton1Down:Connect(function()
        hitboxDragging = true
    end)
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            hitboxDragging = false
        end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if hitboxDragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local mousePos = UserInputService:GetMouseLocation().X
            local sliderPos = hitboxSliderBg.AbsolutePosition.X
            local sliderSize = hitboxSliderBg.AbsoluteSize.X
            local value = math.clamp((mousePos - sliderPos) / sliderSize, 0, 1)
            hitboxSize = math.floor(value * 100)
            if hitboxSize < 1 then hitboxSize = 1 end
            hitboxSizeLabel.Text = "Size: " .. hitboxSize
            hitboxSliderFill.Size = UDim2.new(value, 0, 1, 0)
            hitboxSliderBtn.Position = UDim2.new(value, -10, 0.5, -10)
