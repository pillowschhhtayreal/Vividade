    btn:SetAttribute("ShortText", tab.text:sub(1, 1))
    local btnInnerShadow = Instance.new("UIStroke")
    btnInnerShadow.Color = Color3.fromRGB(0, 0, 0)
    btnInnerShadow.Thickness = 1
    btnInnerShadow.Transparency = 0.5
