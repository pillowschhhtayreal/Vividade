    hitboxColorStroke.Transparency = 0.7
    hitboxColorStroke.Thickness = 1
    hitboxColorStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    hitboxColorStroke.Parent = hitboxColorDropdown
    local hitboxColorMenu = Instance.new("Frame")
    hitboxColorMenu.Size = UDim2.new(1, -160, 0, 0)
    hitboxColorMenu.Position = UDim2.new(0, 150, 0, 30)
    hitboxColorMenu.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    hitboxColorMenu.BorderSizePixel = 0
    hitboxColorMenu.ClipsDescendants = true
    hitboxColorMenu.Visible = false
    hitboxColorMenu.ZIndex = 10
    hitboxColorMenu.Parent = hitboxColorFrame
    local hitboxColorMenuCorner = Instance.new("UICorner")
    hitboxColorMenuCorner.CornerRadius = UDim.new(0, 6)
    hitboxColorMenuCorner.Parent = hitboxColorMenu
    local hitboxColorMenuStroke = Instance.new("UIStroke")
    hitboxColorMenuStroke.Color = Color3.fromRGB(70, 100, 255)
    hitboxColorMenuStroke.Transparency = 0.7
    hitboxColorMenuStroke.Thickness = 1
    hitboxColorMenuStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    hitboxColorMenuStroke.Parent = hitboxColorMenu
    local hitboxColorLayout = Instance.new("UIListLayout")
    hitboxColorLayout.SortOrder = Enum.SortOrder.LayoutOrder
    hitboxColorLayout.Parent = hitboxColorMenu
    local hitboxColorOpen = false
    for _, colorData in ipairs(hitboxColors) do
        local colorButton = Instance.new("TextButton")
        colorButton.Size = UDim2.new(1, 0, 0, 25)
        colorButton.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
        colorButton.BackgroundTransparency = 0.3
        colorButton.Text = colorData.name
        colorButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        colorButton.Font = Enum.Font.GothamMedium
        colorButton.TextSize = 11
        colorButton.TextXAlignment = Enum.TextXAlignment.Left
        colorButton.AutoButtonColor = false
        colorButton.ZIndex = 11
        colorButton.Parent = hitboxColorMenu
        local colorBtnPadding = Instance.new("UIPadding")
        colorBtnPadding.PaddingLeft = UDim.new(0, 10)
        colorBtnPadding.Parent = colorButton
        colorButton.MouseEnter:Connect(function()
            colorButton.BackgroundTransparency = 0
        end)
        colorButton.MouseLeave:Connect(function()
            colorButton.BackgroundTransparency = 0.3
        end)
        colorButton.MouseButton1Click:Connect(function()
            selectedColorName = colorData.name
            hitboxColorDropdown.Text = colorData.name .. "  ▼"
            updateHitboxColor(colorData.color)
            TweenService:Create(hitboxColorMenu, TweenInfo.new(0.2), {Size = UDim2.new(1, -160, 0, 0)}):Play()
            task.wait(0.2)
            hitboxColorMenu.Visible = false
            hitboxColorOpen = false
        end)
    end
    hitboxColorDropdown.MouseButton1Click:Connect(function()
        hitboxColorOpen = not hitboxColorOpen
        if hitboxColorOpen then
            hitboxColorMenu.Visible = true
            TweenService:Create(hitboxColorMenu, TweenInfo.new(0.2), {Size = UDim2.new(1, -160, 0, #hitboxColors * 25)}):Play()
        else
            TweenService:Create(hitboxColorMenu, TweenInfo.new(0.2), {Size = UDim2.new(1, -160, 0, 0)}):Play()
            task.wait(0.2)
            hitboxColorMenu.Visible = false
        end
    end)
    local hbTargetFrame = Instance.new("Frame")
    hbTargetFrame.Size = UDim2.new(1, 0, 0, 70)
    hbTargetFrame.BackgroundTransparency = 1
    hbTargetFrame.Parent = scrollFrame
    hbTargetFrame.ZIndex = 3
    local hbTargetLabel = Instance.new("TextLabel")
    hbTargetLabel.Size = UDim2.new(0, 150, 0, 30)
    hbTargetLabel.BackgroundTransparency = 1
    hbTargetLabel.Text = "Target Player:"
    hbTargetLabel.TextColor3 = Color3.fromRGB(220, 220, 255)
    hbTargetLabel.Font = Enum.Font.GothamMedium
    hbTargetLabel.TextSize = 14
    hbTargetLabel.TextXAlignment = Enum.TextXAlignment.Left
    hbTargetLabel.ZIndex = 3
    hbTargetLabel.Parent = hbTargetFrame
    local function getHBTargetText()
        for _, t in ipairs(hitboxTargetPlayers) do
            if t:lower() == "all" then return "all  ▼" end
        end
        if #hitboxTargetPlayers == 1 then return hitboxTargetPlayers[1] .. "  ▼" end
        return #hitboxTargetPlayers .. " players  ▼"
    end
    local hbTargetDropdown = Instance.new("TextButton")
    hbTargetDropdown.Size = UDim2.new(1, -160, 0, 28)
    hbTargetDropdown.Position = UDim2.new(0, 150, 0, 0)
    hbTargetDropdown.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
    hbTargetDropdown.Text = getHBTargetText()
    hbTargetDropdown.TextColor3 = Color3.fromRGB(255, 255, 255)
    hbTargetDropdown.Font = Enum.Font.GothamMedium
    hbTargetDropdown.TextSize = 12
    hbTargetDropdown.TextXAlignment = Enum.TextXAlignment.Left
    hbTargetDropdown.AutoButtonColor = false
    hbTargetDropdown.ZIndex = 3
    hbTargetDropdown.Parent = hbTargetFrame
    local hbTdPad = Instance.new("UIPadding") hbTdPad.PaddingLeft = UDim.new(0,10) hbTdPad.Parent = hbTargetDropdown
    local hbTdCorner = Instance.new("UICorner") hbTdCorner.CornerRadius = UDim.new(0,6) hbTdCorner.Parent = hbTargetDropdown
    local hbTdStroke = Instance.new("UIStroke") hbTdStroke.Color = Color3.fromRGB(70,100,255) hbTdStroke.Transparency = 0.7 hbTdStroke.Thickness = 1 hbTdStroke.Parent = hbTargetDropdown
    local hbTargetInfo = Instance.new("TextLabel")
    hbTargetInfo.Size = UDim2.new(1, -160, 0, 15)
    hbTargetInfo.Position = UDim2.new(0, 150, 0, 32)
    hbTargetInfo.BackgroundTransparency = 1
    hbTargetInfo.Text = "Shift+Click to select multiple"
    hbTargetInfo.TextColor3 = Color3.fromRGB(150, 150, 170)
    hbTargetInfo.Font = Enum.Font.Gotham
    hbTargetInfo.TextSize = 10
    hbTargetInfo.TextXAlignment = Enum.TextXAlignment.Left
    hbTargetInfo.ZIndex = 3
    hbTargetInfo.Parent = hbTargetFrame
    local hbTargetMenu = Instance.new("ScrollingFrame")
    hbTargetMenu.Size = UDim2.new(1, -160, 0, 0)
    hbTargetMenu.Position = UDim2.new(0, 150, 0, 50)
    hbTargetMenu.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    hbTargetMenu.BorderSizePixel = 0
    hbTargetMenu.ClipsDescendants = true
    hbTargetMenu.Visible = false
    hbTargetMenu.ScrollBarThickness = 4
    hbTargetMenu.ZIndex = 20
    hbTargetMenu.Parent = hbTargetFrame
    local hbTmCorner = Instance.new("UICorner") hbTmCorner.CornerRadius = UDim.new(0,6) hbTmCorner.Parent = hbTargetMenu
    local hbTmStroke = Instance.new("UIStroke") hbTmStroke.Color = Color3.fromRGB(70,100,255) hbTmStroke.Transparency = 0.7 hbTmStroke.Thickness = 1 hbTmStroke.Parent = hbTargetMenu
    local hbTmLayout = Instance.new("UIListLayout") hbTmLayout.SortOrder = Enum.SortOrder.LayoutOrder hbTmLayout.Parent = hbTargetMenu
    local hbTargetOpen = false
    local hbTargetBtns = {}
    local function isHBTargetedLocal(name)
        for _, t in ipairs(hitboxTargetPlayers) do
            if t:lower() == name:lower() then return true end
        end
        return false
    end
    local function populateHBTargetList()
        for _, c in ipairs(hbTargetMenu:GetChildren()) do
            if c:IsA("TextButton") then c:Destroy() end
        end
        hbTargetBtns = {}
        local allItems = {{name = "all", display = "all"}}
        for _, plr in ipairs(Players:GetPlayers()) do
            if plr ~= player then
                table.insert(allItems, {name = plr.Name, display = plr.DisplayName .. " (" .. plr.Name .. ")"})
            end
        end
        for _, item in ipairs(allItems) do
            local btn = Instance.new("TextButton")
            btn.Size = UDim2.new(1, 0, 0, 26)
            btn.BackgroundColor3 = isHBTargetedLocal(item.name) and Color3.fromRGB(70,100,255) or Color3.fromRGB(30,30,40)
            btn.BackgroundTransparency = isHBTargetedLocal(item.name) and 0.2 or 0.3
            btn.Text = item.display
            btn.TextColor3 = Color3.fromRGB(255, 255, 255)
            btn.Font = Enum.Font.GothamMedium
            btn.TextSize = 11
            btn.TextXAlignment = Enum.TextXAlignment.Left
            btn.AutoButtonColor = false
            btn.ZIndex = 21
            btn.Parent = hbTargetMenu
            local p = Instance.new("UIPadding") p.PaddingLeft = UDim.new(0,10) p.Parent = btn
            hbTargetBtns[item.name] = btn
            btn.MouseButton1Click:Connect(function()
                local shift = UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) or UserInputService:IsKeyDown(Enum.KeyCode.RightShift)
                if item.name == "all" then
                    hitboxTargetPlayers = {"all"}
                    for n, b in pairs(hbTargetBtns) do
                        b.BackgroundColor3 = n == "all" and Color3.fromRGB(70,100,255) or Color3.fromRGB(30,30,40)
                        b.BackgroundTransparency = n == "all" and 0.2 or 0.3
                    end
                elseif shift then
                    for i, t in ipairs(hitboxTargetPlayers) do
                        if t == "all" then table.remove(hitboxTargetPlayers, i) break end
                    end
                    local found = false
                    for i, t in ipairs(hitboxTargetPlayers) do
                        if t == item.name then table.remove(hitboxTargetPlayers, i) found = true break end
                    end
                    if not found then table.insert(hitboxTargetPlayers, item.name) end
                    if #hitboxTargetPlayers == 0 then hitboxTargetPlayers = {"all"} end
                    for n, b in pairs(hbTargetBtns) do
                        local sel = isHBTargetedLocal(n)
                        b.BackgroundColor3 = sel and Color3.fromRGB(70,100,255) or Color3.fromRGB(30,30,40)
                        b.BackgroundTransparency = sel and 0.2 or 0.3
                    end
                else
                    hitboxTargetPlayers = {item.name}
                    for n, b in pairs(hbTargetBtns) do
                        b.BackgroundColor3 = n == item.name and Color3.fromRGB(70,100,255) or Color3.fromRGB(30,30,40)
                        b.BackgroundTransparency = n == item.name and 0.2 or 0.3
                    end
                end
                hbTargetDropdown.Text = getHBTargetText()
                updateHitboxes()
            end)
        end
        hbTargetMenu.CanvasSize = UDim2.new(0, 0, 0, hbTmLayout.AbsoluteContentSize.Y)
    end
    hbTargetDropdown.MouseButton1Click:Connect(function()
        hbTargetOpen = not hbTargetOpen
        if hbTargetOpen then
            populateHBTargetList()
            hbTargetMenu.Visible = true
            local h = math.min(150, #Players:GetPlayers() * 26 + 26)
            TweenService:Create(hbTargetMenu, TweenInfo.new(0.2), {Size = UDim2.new(1,-160,0,h)}):Play()
        else
            TweenService:Create(hbTargetMenu, TweenInfo.new(0.2), {Size = UDim2.new(1,-160,0,0)}):Play()
            task.wait(0.2)
            hbTargetMenu.Visible = false
        end
    end)
    local giverTitle = Instance.new("TextLabel")
    giverTitle.Size = UDim2.new(1, 0, 0, 40)
    giverTitle.BackgroundTransparency = 1
    giverTitle.Text = "Weapon Giver"
    giverTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    giverTitle.Font = Enum.Font.GothamBold
    giverTitle.TextSize = 18
    giverTitle.TextXAlignment = Enum.TextXAlignment.Left
    giverTitle.ZIndex = 3
    giverTitle.Parent = scrollFrame
    local giverBtnFrame = Instance.new("Frame")
    giverBtnFrame.Size = UDim2.new(1, 0, 0, 35)
    giverBtnFrame.BackgroundTransparency = 1
    giverBtnFrame.Parent = scrollFrame
    giverBtnFrame.ZIndex = 3
    local giverTurnOnBtn = Instance.new("TextButton")
    giverTurnOnBtn.Size = UDim2.new(1, 0, 0, 30)
    giverTurnOnBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
    giverTurnOnBtn.Text = "Turn On"
    giverTurnOnBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    giverTurnOnBtn.Font = Enum.Font.GothamBold
    giverTurnOnBtn.TextSize = 14
    giverTurnOnBtn.AutoButtonColor = false
    giverTurnOnBtn.ZIndex = 3
    giverTurnOnBtn.Parent = giverBtnFrame
    local giverBtnCorner = Instance.new("UICorner")
    giverBtnCorner.CornerRadius = UDim.new(0, 6)
    giverBtnCorner.Parent = giverTurnOnBtn
    local giverBtnStroke = Instance.new("UIStroke")
    giverBtnStroke.Color = Color3.fromRGB(70, 100, 255)
    giverBtnStroke.Transparency = 0.5
    giverBtnStroke.Thickness = 1
    giverBtnStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    giverBtnStroke.Parent = giverTurnOnBtn
    giverTurnOnBtn.MouseEnter:Connect(function()
        if not giverEnabled then
            giverTurnOnBtn.BackgroundColor3 = Color3.fromRGB(70, 100, 255)
        end
    end)
    giverTurnOnBtn.MouseLeave:Connect(function()
        if not giverEnabled then
            giverTurnOnBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
        end
    end)
    giverTurnOnBtn.MouseButton1Click:Connect(function()
        if giverEnabled then return end
        giverEnabled = true
        giverTurnOnBtn.Text = "Active ✓"
        giverTurnOnBtn.BackgroundColor3 = Color3.fromRGB(70, 100, 255)
        player.CharacterAdded:Connect(function()
            player.PlayerGui:WaitForChild("Menu Screen"):WaitForChild("RemoteEvent"):FireServer("Emerald Greatsword")
            player.PlayerGui["Menu Screen"].Enabled = false
        end)
        if player.PlayerGui:FindFirstChild("Menu Screen") then
            local ms = player.PlayerGui["Menu Screen"]
            if ms:FindFirstChild("RemoteEvent") then
                ms.RemoteEvent:FireServer("Emerald Greatsword")
                ms.Enabled = false
            end
        end
    end)
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 40)
end
local function createPlaceholderContent(tabName)
    clearContentArea()
    local placeholder = Instance.new("TextLabel")
    placeholder.Size = UDim2.new(1, -20, 1, -20)
    placeholder.Position = UDim2.new(0, 10, 0, 10)
    placeholder.BackgroundTransparency = 1
    placeholder.Text = tabName .. " Tab\n\nThis tab is under construction."
    placeholder.TextColor3 = Color3.fromRGB(255, 255, 255)
    placeholder.Font = Enum.Font.GothamMedium
    placeholder.TextSize = 16
    placeholder.TextWrapped = true
    placeholder.TextXAlignment = Enum.TextXAlignment.Center
    placeholder.TextYAlignment = Enum.TextYAlignment.Center
    placeholder.ZIndex = 2
    placeholder.Parent = content
end
local function selectTab(tabName)
    if selectedButton then
        TweenService:Create(selectedButton, TweenInfo.new(0.2), {
            BackgroundTransparency = 0.4,
            Size = UDim2.new(1, -16, 0, 40)
        }):Play()
        local prevGlow = selectedButton:FindFirstChildWhichIsA("UIStroke")
        if prevGlow then
            TweenService:Create(prevGlow, TweenInfo.new(0.2), {
                Transparency = 0.9
            }):Play()
        end
    end
    local currentButton = tabButtons[tabName]
    if currentButton then
        TweenService:Create(currentButton, TweenInfo.new(0.2), {
            BackgroundTransparency = 0.1,
            Size = UDim2.new(1, -12, 0, 42)
        }):Play()
        local currentGlow = currentButton:FindFirstChildWhichIsA("UIStroke")
        if currentGlow then
            TweenService:Create(currentGlow, TweenInfo.new(0.2), {
                Transparency = 0.6
            }):Play()
        end
        selectedButton = currentButton
        currentTab = tabName
    end
    if tabName == "Home" then
        createHomeTabContent()
    elseif tabName == "Players" then
        createPlayersTabContent()
    elseif tabName == "Movement" then
        createMovementTabContent()
    elseif tabName == "Client" then
        createClientTabContent()
    elseif tabName == "Combat" then
        createMiscTabContent()
    elseif tabName == "Settings" then
        createSettingsTabContent()
    elseif tabName == "Info" then
        createInfoTabContent()
    else
        createPlaceholderContent(tabName)
    end
end
for tabName, button in pairs(tabButtons) do
    button.MouseButton1Click:Connect(function()
        selectTab(tabName)
    end)
end
toggleSidebarBtn.MouseButton1Click:Connect(function()
    isSidebarOpen = not isSidebarOpen
    if isSidebarOpen then
        TweenService:Create(sidebar, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {Size = UDim2.new(0,220,1,-45)}):Play()
        toggleSidebarBtn.Text = "◄"
        for _, btn in pairs(tabButtons) do
            btn.Text = btn:GetAttribute("FullText")
        end
    else
        TweenService:Create(sidebar, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {Size = UDim2.new(0,64,1,-45)}):Play()
        toggleSidebarBtn.Text = "►"
        for _, btn in pairs(tabButtons) do
            btn.Text = btn:GetAttribute("ShortText")
        end
    end
    updateContentPosition()
end)
closeBtn.MouseButton1Click:Connect(function()
    main.Visible = false
    reopenButton.Visible = true
end)
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == openGuiHotkey and scriptEnabled then
        if main.Visible then
            main.Visible = false
            reopenButton.Visible = true
        else
            main.Visible = true
            reopenButton.Visible = false
        end
    end
end)
local dragConnection = nil
titleBar.Active = true
titleBar.Selectable = true
local currentTween = nil
titleBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        local initialPosition = input.Position
        local initialMainPosition = main.Position
        dragConnection = UserInputService.InputChanged:Connect(function(input2)
            if input2.UserInputType == Enum.UserInputType.MouseMovement or input2.UserInputType == Enum.UserInputType.Touch then
                local currentPosition = input2.Position
                local delta = currentPosition - initialPosition
                local newPosition = UDim2.new(
                    initialMainPosition.X.Scale,
                    initialMainPosition.X.Offset + delta.X,
                    initialMainPosition.Y.Scale,
                    initialMainPosition.Y.Offset + delta.Y
                )
                if currentTween then
                    currentTween:Cancel()
                end
                currentTween = TweenService:Create(main, TweenInfo.new(0.1, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
                    Position = newPosition
                })
                currentTween:Play()
            end
        end)
    end
end)
UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        if dragConnection then
            dragConnection:Disconnect()
            dragConnection = nil
        end
        if currentTween then
            currentTween:Cancel()
            currentTween = nil
        end
    end
end)
selectTab("Home")

